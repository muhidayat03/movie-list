{
  "name": "movie-list",
  "version": "1.0.0",
  "description": "movie list website",
  "main": "index.js",
  "scripts": {
    "start": "node build/bundle.js",
    "lint": "eslint",
    "heroku-postbuild": "npm install && npm run build:prod",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "build:prod": "rm -rf build && rm -rf public && webpack  --config webpack.server.js && webpack  --config webpack.client.js --mode production"
  },
  "keywords": [],
  "author": "muhidayat03",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^3.4.3",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1",
    "styled-components": "^5.2.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-plugin-react": "^7.21.5"
  }
}
